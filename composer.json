{
  "name": "lokalnyi/loki",
  "description": "PHP SDK for conversion rate enhancement technology and adaptive response to user activity on a web page.",
  "version": "0.0.1",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.2",
    "mobiledetect/mobiledetectlib": "^3.74",
    "predis/predis": "^2.1",
    "nesbot/carbon": "^2.71",
    "illuminate/support": "^8.83",
    "illuminate/http": "^8.83",
    "illuminate/validation": "^8.83"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "Loki\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Loki\\": "src"
    }
  },
  "authors": [
    {
      "name": "Ivan Homeniuk",
      "email": "ggommenn@gmail.com"
    }
  ],
  "minimum-stability": "stable"
}
